{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Suicide Deaths in Australia (2020)",
  "width": 800,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/collc07/Week9/main/js/ne_10m_admin_1_states_provinces.topojson",
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
  },
  "view": {"fill": "#F3FAF9"},
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/collc07/Week9/main/data/suicide2020_map_final.csv"
        },
        "key": "State",
        "fields": ["Deaths"]
      }
    },
    {"calculate": "datum.Deaths", "as": "Total Deaths"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Total Deaths",
      "type": "quantitative",
      "scale": {"scheme": "reds"}
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "State"},
      {"field": "Total Deaths", "type": "quantitative"}
    ]
  }
}